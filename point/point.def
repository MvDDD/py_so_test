struct Point {
    f32 x;
    f32 y;
    f32 vx;
    f32 vy;
    i32 error;
}

func Point_rotate(p:ptr<Point>, center:ptr<Point>, angle:f32) -> void
func Point_update(p:ptr<Point>) -> void
func Point_create(p:ptr<Point>) -> void
func Point_destroy(p:ptr<Point>) -> void
func Point__error(p:ptr<Point>) -> i32
func Point_throw(p:ptr<Point>, code:i32) -> void
